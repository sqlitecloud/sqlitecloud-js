{
  "name": "sqlitecloud-js",
  "version": "0.0.25",
  "description": "SQLiteCloud drivers for Typescript/Javascript in edge, web and node clients",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --coverage",
    "build": "rm -rf ./lib/ && tsc --project tsconfig.build.json",
    "prettier": "prettier --write 'src/**/*'",
    "lint": "eslint ./src/ --fix && tsc --noEmit",
    "typedoc": "rm -rf ./docs/ && typedoc --out docs && typedoc --plugin typedoc-plugin-markdown --out docs/markdown",
    "npmgui": "npx npm-gui@latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sqlitecloud/sqlitecloud-js.git"
  },
  "license": "MIT",
  "author": {
    "name": "SQLiteCloud, Inc.",
    "email": "support@sqlitecloud.io",
    "url": "https://sqlitecloud.io/"
  },
  "engines": {
    "node": ">=18.0"
  },
  "keywords": [
    "sql",
    "sqlite",
    "sqlite3",
    "sqlitecloud",
    "database",
    "cloud"
  ],
  "bugs": {
    "url": "https://github.com/sqlitecloud/sqlitecloud-js/issues"
  },
  "homepage": "https://github.com/sqlitecloud/sqlitecloud-js#readme",
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "lz4js": "^0.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/lz4": "^0.6.3",
    "@types/node": "^12.20.55",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "dotenv": "^16.3.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.2",
    "sqlite3": "^5.1.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.3",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^4.9.5"
  },
  "config": {},
  "release": {
    "branches": [
      "main"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "printWidth": 160
  }
}
